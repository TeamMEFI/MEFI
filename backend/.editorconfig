# 최상위에 있는 파일로 명시
root = true

# 모든 파일
[*]

# [encoding-utf8]
charset = utf-8

# [newline-lf]
end_of_line = lf

# [newline-eof]
insert_final_newline = true

# 자바 파일
[*.java]

# [indentation-tab]
indent_style = tab

# [4-spaces-tab]
indent_size = 4
tab_width = 4

# [no-trailing-spaces]
trim_trailing_whitespace = true

# [line-length-120]
max_line_length = 120